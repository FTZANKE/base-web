{
  "name": "base-web",
  "private": true,
  "version": "0.0.1",
  "homepage": "https://ftzanke.github.io/base-web/",
  "scripts": {
    "dev": "vite --mode=development --force",
    "build": "vue-tsc && vite build --mode=production",
    "preview": "vite preview --mode=production --force",
    "lint:prettier": "prettier --write .",
    "lint:eslint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:stylelint": "stylelint \"**/*.{vue,css,scss,html}\"  --fix --cache --cache-location node_modules/.cache/stylelint/ --allow-empty-input",
    "lint:all": "pnpm run lint:prettier && pnpm run lint:eslint && pnpm run lint:stylelint",
    "tsc": "vue-tsc --noEmit",
    "husky": "husky install",
    "init": "pnpm install && pnpm run husky",
    "analysis": "windicss-analysis",
    "deploy": "vue-tsc && vite build --mode=production && gh-pages -d dist"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=7.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "element-plus": "^2.3.4",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vxe-table": "^4.3.12",
    "xe-utils": "^3.5.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-standard": "^8.0.1",
    "@windicss/plugin-animations": "^1.0.9",
    "@windicss/plugin-question-mark": "^0.1.1",
    "@windicss/plugin-scrollbar": "^1.2.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.23",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "terser": "^5.17.1",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-element-plus": "^0.7.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.10",
    "vue-tsc": "^1.6.4",
    "windicss": "^3.5.6",
    "windicss-analysis": "^0.3.5"
  }
}
